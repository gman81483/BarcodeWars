			android:layout_height="wrap_content"
			android:layout_width="wrap_content"
			android:layout_alignParentRight="true"
			android:layout_below="@id/TextViewTank"
			android:maxHeight="5dip">
		</SeekBar>
	</RelativeLayout>
	<RelativeLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	android:layout_width="fill_parent"
	android:layout_height="wrap_content"
	android:padding="6dip"
	android:id="@+id/RelativeLayoutHeli">
		
		<ImageView
			android:id="@+id/ImageViewHeli"
			android:src="@drawable/heli"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_alignParentTop="true"
			android:layout_alignParentLeft="true"
			android:layout_marginRight="6dip">
		</ImageView>
		<TextView
			android:id="@+id/TextViewHeli"
			android:text="Helicopter"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_alignParentTop="true"
			android:layout_toRightOf="@id/ImageViewHeli">
		</TextView>
		<TextView
			android:id="@+id/TextViewHeliQuantity"
			android:text="0"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_alignParentTop="true"
			android:layout_alignParentRight="true"
			android:layout_marginLeft="3dip">
		</TextView>
		<TextView
			android:id="@+id/TextViewHeliSlash"
			android:text="/"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_alignParentTop="true"
			android:layout_toLeftOf="@id/TextViewHeliQuantity"
			android:layout_marginLeft="3dip">
		</TextView>
		<TextView
			android:id="@+id/TextViewHeliSelected"
			android:text="0"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_alignParentTop="true"
			android:layout_toLeftOf="@id/TextViewHeliSlash">
		</TextView>
		<SeekBar
			android:id="@+id/SeekBarHeli"
			android:layout_toRightOf="@id/ImageViewHeli"
			android:layout_height="wrap_content"
			android:layout_width="wrap_content"
			android:layout_alignParentRight="true"
			android:layout_below="@id/TextViewHeli"
			android:maxHeight="5dip">
		</SeekBar>
	</RelativeLayout>
	<RelativeLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	android:layout_width="fill_parent"
	android:layout_height="wrap_content"
	android:padding="6dip"
	android:id="@+id/RelativeLayoutJet">
		
		<ImageView
			android:id="@+id/ImageViewJet"
			android:src="@drawable/f18"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_alignParentTop="true"
			android:layout_alignParentLeft="true"
			android:layout_marginRight="6dip">
		</ImageView>
		<TextView
			android:id="@+id/TextViewJet"
			android:text="Fighter Jet"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_alignParentTop="true"
			android:layout_toRightOf="@id/ImageViewJet">
		</TextView>
		<TextView
			android:id="@+id/TextViewJetQuantity"
			android:text="0"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_alignParentTop="true"
			android:layout_alignParentRight="true"
			android:layout_marginLeft="3dip">
		</TextView>
		<TextView
			android:id="@+id/TextViewJetSlash"
			android:text="/"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_alignParentTop="true"
			android:layout_toLeftOf="@id/TextViewJetQuantity"
			android:layout_marginLeft="3dip">
		</TextView>
		<TextView
			android:id="@+id/TextViewJetSelected"
			android:text="0"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_alignParentTop="true"
			android:layout_toLeftOf="@id/TextViewJetSlash">
		</TextView>
		<SeekBar
			android:id="@+id/SeekBarJet"
			android:layout_toRightOf="@id/ImageViewJet"
			android:layout_height="wrap_content"
			android:layout_width="wrap_content"
			android:layout_alignParentRight="true"
			android:layout_below="@id/TextViewJet"
			android:maxHeight="5dip">
		</SeekBar>
	</RelativeLayout>
</LinearLayout>
